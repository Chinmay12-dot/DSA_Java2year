public class Queue {

    Node head;

    class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
        }
    }

    void addFirst(int data) {
        Node newNode = new Node(data);
        newNode.next = head;
        head = newNode;
    }

    void deleteFirst() {
        if (head == null) {
            System.out.println("List is already empty.");
            return;
        }
        head = head.next;
    }

    void printList() {
        Node curr = head;
        System.out.print("Queue: [");
        while (curr != null) {
            System.out.print(curr.data);
            if (curr.next != null) {
                System.out.print(", ");
            }
            curr = curr.next;
        }
        System.out.println("]");
    }

    public static void main(String[] args) {
        Queue stack = new Queue();

        stack.printList();

        stack.addFirst(10);
        stack.addFirst(20);
        stack.addFirst(30);
        stack.addFirst(40);
        stack.printList(); 

        stack.deleteFirst();
        stack.printList(); 

        stack.deleteFirst();
        stack.printList();
    }
}
